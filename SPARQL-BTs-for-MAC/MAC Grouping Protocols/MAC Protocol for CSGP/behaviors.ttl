:RP_InitiatorSendRequestBT
	a bt:BehaviorTree ;
	a bt:Root ;
	rdfs:label "RP-Initiator Send Request Behavior Tree" ;
	bt:hasChild [
			a bt:Priority ;
			bt:hasChildren (
				[
					a bt:Sequence ;
					bt:hasChildren (
						:HandleInitSendRequestGoal
						:WriteThisAgentIdToEKB
						:WriteParticipantAddressesToEKB
						:MessageToParticipants
						:SaveRequest
						[
							a bt:UntilSuccess ;
							bt:hasChild [
								a bt:Priority ;
								bt:hasChildren (
									[
										a bt:Sequence ;
										bt:hasChildren (
											:GetResponses
											[
												a bt:Priority ;
												bt:hasChildren (
													[
														a bt:Invert ;
														bt:hasChild 
															:NotificationNecessary
													]
													:AgreementsReached
												)
											]
											:QuorumReached
										)	
									]
									:TimeoutReached
								)
							]
						]
						:ClearEKB
					)
				]
				[
					a bt:Invert ;
					bt:hasChild 
						:ClearEKB
				]
		)
				
	] .

